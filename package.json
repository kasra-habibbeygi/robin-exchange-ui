{
	"name": "robin-exchange-ui",
	"version": "1.0.22",
	"description": "A simple library for React components.",
	"author": "Kasra Habibbeygi",
	"license": "MIT",
	"type": "module",
	"main": "dist/core/index.js",
	"module": "dist/core/index.esm.js",
	"types": "dist/core/index.d.ts",
	"exports": {
		"./core": {
			"import": "./dist/core/index.esm.js",
			"require": "./dist/core/index.js",
			"types": "./dist/core/index.d.ts"
		},
		"./hooks": {
			"import": "./dist/hooks/index.esm.js",
			"require": "./dist/hooks/index.js",
			"types": "./dist/hooks/index.d.ts"
		},
		"./icons": {
			"import": "./dist/icons/index.esm.js",
			"require": "./dist/icons/index.js",
			"types": "./dist/icons/index.d.ts"
		},
		"./typography": {
			"import": "./dist/typography/index.esm.js",
			"require": "./dist/typography/index.js",
			"types": "./dist/typography/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "yarn clean && rollup -c",
		"prepare": "husky",
		"check-lint": "eslint . --ext ts --ext tsx --ext js",
		"svgr": "rimraf ./src/icons/main && svgr -d ./src/icons/main ./src/icons/svg --typescript --icon && node scripts/svgr-helper.js ",
		"check-types": "tsc --pretty --noEmit",
		"icon-linter": "eslint './src/icons/main/**/*.{ts,tsx}' --fix && eslint './src/icons/list.tsx' --fix",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"clean": "rimraf dist"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "1.3.1",
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@eslint/eslintrc": "^3.0.2",
		"@eslint/js": "^9.0.0",
		"@mdx-js/react": "^3.0.1",
		"@mui/material": "^5.15.16",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-replace": "^5.0.7",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@storybook/addon-docs": "^8.1.4",
		"@storybook/addon-essentials": "^8.0.6",
		"@storybook/addon-interactions": "^8.0.6",
		"@storybook/addon-links": "^8.0.6",
		"@storybook/addon-onboarding": "^8.0.6",
		"@storybook/addon-themes": "^8.0.6",
		"@storybook/blocks": "^8.0.6",
		"@storybook/react": "^8.1.4",
		"@storybook/react-vite": "^8.0.6",
		"@storybook/test": "^8.0.6",
		"@svgr/cli": "^8.1.0",
		"@types/add": "^2",
		"@types/react": "^18.2.74",
		"@types/react-dom": "^18",
		"@types/rollup-plugin-peer-deps-external": "^2",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^7.5.0",
		"add": "^2.0.6",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"eslint-plugin-storybook": "^0.8.0",
		"globals": "^15.0.0",
		"husky": "^9.0.11",
		"prettier": "^3.2.5",
		"prettier-eslint": "^16.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.52.1",
		"react-router-dom": "^6.24.0",
		"react-select": "^5.8.0",
		"rimraf": "^5.0.7",
		"rollup": "^4.14.1",
		"rollup-plugin-dts": "^6.1.0",
		"rollup-plugin-minification": "^0.2.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"storybook": "^8.0.6",
		"svgr": "^2.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.4.4",
		"typescript-eslint": "^7.5.0",
		"vite": "^5.2.11"
	},
	"peerDependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@mui/material": "^5.15.16",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^6.24.0"
	}
}
